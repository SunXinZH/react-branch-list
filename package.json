    "react": "^19.0.0",
    "vsc-base-kits": "^1.0.6"
  },
  "devDependencies": {
    "@types/node": "^22.10.10",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "tsc": "^2.0.4",
    "typescript": "^5.7.3"
  }
}